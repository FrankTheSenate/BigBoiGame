    vspd= 0;
    
    //Jumping
    if (jkey) {
    vspd = -jspd
    audio_play_sound(Jump_Sound, 10, false);
    }
}   else  {
      // Gravity
      if (vspd &lt; 18) {
      vspd += grav;
      }
      if (keyboard_check_released(ord("W")) and vspd &lt;-4){
      vspd = -4
      }
}
// Moving to the right
if (rkey){
hspd = spd;

    //Wall Jump Left
    if (place_meeting(x-1, y, obj_wood) and !place_meeting(x, y+1 , obj_wood) and !lkey and jkey)
        {
        vspd = -jspd;
        audio_play_sound(Jump_Sound, 10, false)
sprite_index = spr_nick_bullet_jump_left
    }
}
/// Moving to the left
if (lkey) {
hspd = -spd
if (place_meeting (x+1, y, obj_wood) and !place_meeting(x, y+1, obj_wood) and !rkey and jkey){
vspd = -jspd
audio_play_sound(Jump_Sound, 10, false)
sprite_index = spr_nick_bullet_jump_right
}
}
///Check for not moving
if ((!rkey &amp;&amp; !lkey) || (rkey &amp;&amp; lkey)) {
   hspd = 0
}




if vspd = 0 and hspd = 0
{
global.nick_movement = 0
}
else
{
global.nick_movement = 1
}
